{
  "name": "@okutanidaichi/hang",
  "version": "0.1.2",
  "description": "",
  "main": "dist/index.js",
  "module": "src/index.ts",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run build:main && npm run build:browser",
    "build:main": "tsc",
    "build:browser": "tsc --project tsconfig.browser.json",
    "postbuild": "node ./scripts/copy-src-to-dist.cjs",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "browser": "./dist/browser.js"
    },
    "./catalog": {
      "import": "./dist/catalog/index.js",
      "types": "./dist/catalog/index.d.ts",
      "browser": "./dist/catalog/index.js"
    },
    "./elements": {
      "import": "./dist/elements/index.js",
      "types": "./dist/elements/index.d.ts",
      "browser": "./dist/elements/browser.js"
    },
    "./internal": {
      "import": "./dist/internal/index.js",
      "types": "./dist/internal/index.d.ts",
      "browser": "./dist/internal/browser.js"
    },
    "./media": {
      "import": "./dist/media/index.js",
      "types": "./dist/media/index.d.ts",
      "browser": "./dist/media/browser.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/audioworklet": "^0.0.83",
    "@types/node": "^24.0.4",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "@vitest/coverage-istanbul": "3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.30.1",
    "jsdom": "^27.0.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "web-streams-polyfill": "^4.2.0"
  },
  "dependencies": {
    "@okutanidaichi/moqt": "^0.7.1",
    "golikejs": "^0.4.0",
    "zod": "^4.1.5"
  },
  "packageManager": "pnpm@10.18.3"
}
